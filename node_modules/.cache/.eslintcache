        ~<br/>\"Here For All Your Four-Legged Friend's Needs\"<br/>~\n                </div>  \n                <div className={styles.footer}>\n                    Utilized with <em><a href=\"https://github.com/tuftsm/pets.git\" target=\"_blank\">Github Repo</a></em>&emsp;&emsp;&emsp;\n                    All non-Matthew images licensed for use with <em>Creative Commons</em> licensing. The creator of this website does not claim to own any non-Matthew images.<br/>\n                </div>   \n              </div>\n  );\n}\n\nexport default Login;","/usr/share/caddy/firebase/src/pages/CreatePost.js",["59","60"],[],"import React, { useState, useEffect } from \"react\";\nimport { addDoc, collection } from \"firebase/firestore\";\nimport { db, auth } from \"../firebase-config\";\nimport { useNavigate } from \"react-router-dom\";\nimport styles from './styles.module.css';\n\nfunction CreatePost({ isAuth }) {\n  const [title, setTitle] = useState(\"\");\n  const [postText, setPostText] = useState(\"\");\n\n  const postsCollectionRef = collection(db, \"posts\");\n  let navigate = useNavigate();\n\n  const createPost = async () => {\n    await addDoc(postsCollectionRef, {\n      title,\n      postText,\n      author: { name: auth.currentUser.displayName, id: auth.currentUser.uid },\n    });\n    navigate(\"/\");\n  };\n\n  useEffect(() => {\n    if (!isAuth) {\n      navigate(\"/login\");\n    }\n  }, []);\n\n  return (\n    <div>\n    <div className=\"createPostPage\">\n      <div className=\"cpContainer\">\n        <h1>Review Matthew's Pet Services</h1>\n        <div className=\"inputGp\">\n          <label> Pet's Name:</label>\n          <input\n            placeholder=\"Name...\"\n            onChange={(event) => {\n              setTitle(event.target.value);\n            }}\n          />\n        </div>\n        <div className=\"inputGp\">\n          <label> Post:</label>\n          <textarea\n            placeholder=\"Share experience...\"\n            onChange={(event) => {\n              setPostText(event.target.value);\n            }}\n          />\n        </div>\n        <button onClick={createPost}> Submit Post</button>\n      </div>\n    </div>\n                <div className={styles.home}>\n                    ~<br/>\"Here For All Your Four-Legged Friend's Needs\"<br/>~\n                </div>  \n                <div className={styles.footer}>\n                    Utilized with <em><a href=\"https://github.com/tuftsm/pets.git\" target=\"_blank\">Github Repo</a></em>&emsp;&emsp;&emsp;\n                    All non-Matthew images licensed for use with <em>Creative Commons</em> licensing. The creator of this website does not claim to own any non-Matthew images.<br/>\n                </div>   \n                </div>\n  );\n}\n\nexport default CreatePost;","/usr/share/caddy/firebase/src/pages/Reviews.js",["61","62"],[],"import React, { useEffect, useState } from \"react\";\nimport { getDocs, collection, deleteDoc, doc } from \"firebase/firestore\";\nimport { auth, db } from \"../firebase-config\";\nimport styles from './styles.module.css';\n\nfunction Home({ isAuth }) {\n  const [postLists, setPostList] = useState([]);\n  const postsCollectionRef = collection(db, \"posts\");\n\n  const getPosts = async () => {\n    try {\n      const data = await getDocs(postsCollectionRef);\n      setPostList(\n        data.docs.map((post) => ({\n          ...post.data(),\n          id: post.id,\n        }))\n      );\n    } catch (err) {\n      console.log(err);\n    }\n  };\n\n  const deletePost = async (id) => {\n    const postDoc = doc(db, \"posts\", id);\n    await deleteDoc(postDoc);\n    getPosts();\n  };\n\n  useEffect(() => {\n    console.log(\"Effect called\");\n    getPosts();\n  }, []);\n  \n  return (\n    <div>\n    <div className=\"homePage\">\n      {postLists.map((post) => {\n        return (\n          <div className=\"post\">\n            <div className=\"postHeader\">\n              <div className=\"title\">\n                <h1> {post.title}</h1>\n              </div>\n              <div className=\"deletePost\">\n                {isAuth && post.author.id === auth.currentUser.uid && (\n                  <button\n                    onClick={() => {\n                      deletePost(post.id);\n                    }}\n                  >\n                    {\" \"}\n                    &#128465;\n                  </button>\n                )}\n              </div>\n            </div>\n            <div className=\"postTextContainer\"> {post.postText} </div>\n            <h3>-- {post.author.name}</h3>\n          </div>\n        );\n      })}\n    </div>\n                <div className={styles.home}>\n                    ~<br/>\"Here For All Your Four-Legged Friend's Needs\"<br/>~\n                </div>  \n                <div className={styles.footer}>\n                    Utilized with <em><a href=\"https://github.com/tuftsm/pets.git\" target=\"_blank\">Github Repo</a></em>&emsp;&emsp;&emsp;\n                    All non-Matthew images licensed for use with <em>Creative Commons</em> licensing. The creator of this website does not claim to own any non-Matthew images.<br/>\n                </div>   \n                </div>\n  );\n}\n\nexport default Home;","/usr/share/caddy/firebase/src/pages/Home.js",["63","64","65","66"],[],"import React from 'react';\nimport { Image, View } from 'react';\nimport ReactDOM from 'react-dom/client';\nimport styles from './styles.module.css';\nimport background from './images/bwHome.jpg'\n\nfunction Home() {\n    return(\n        <home>\n        <div class=\"section\">  \n            <div className={styles.home}>\n                <img \n                    style={{width:'100%'}}\n                    src={background} alt=\"black and white dog walker\"/>\n            </div>\n            <div className={styles.topRight}><em>Only the Best<br/>For Your Good Boy/Girl</em></div>\n        </div>\n        <div className={styles.homefooter}>\n            &emsp;&emsp;&emsp;Utilized with <em><a href=\"https://github.com/tuftsm/pets.git\" target=\"_blank\">Github classroom</a></em>&emsp;&emsp;&emsp;\n            All images licensed for use with <em>Creative Commons</em> licensing. The creator of this website does not claim to own any images.\n        </div>\n        </home>\n    );\n};\n\n\nexport default Home;","/usr/share/caddy/firebase/src/pages/About.js",["67","68","69"],[],"import React from 'react';\nimport ReactDOM from 'react-dom/client';\nimport styles from './styles.module.css';\nimport pic1 from './images/beach.jpg';\nimport pic2 from './images/mewalking.jpg';\nimport pic3 from './images/fronthall.jpg';\nimport pic4 from './images/night.JPG';\n\nfunction About() {\n    return(\n        <home>\n            <div className={styles.about}>\n                <div className={styles.photoGridContainer}>\n                    <div className={styles.photoGrid}>\n                        <img src={pic1} alt=\"bike\" width=\"400\"/>\n                    <div className={styles.bio}>\n                        <strong><br/>About Matthew</strong>\n                    </div>\n                        <br/>Matthew's Pet Services is the direct product of one man's love of dogs. <br/><br/> Since he was a young child, Matthew Tufts has always had a special love for dogs. Unfortunately, as a child Matthew never had the opportunity to take care of a dog of his own. Instead, he found ways to interact with dogs through friends, neighbors, and extended family. <br/><br/>  \n                        <img src={pic3} alt=\"bike\" width=\"400\"/>\n                        <br/> His life changed one day when his family welcomed a baby maltipoo into their home and into their hearts. <br/><br/>Upon learning what it takes to raise a dog, Matthew developed a sympathy for fellow dog owners. What has he done about it? That's where Matthew's Pet Services comes into the picture.<br/><br/>   \n                        <img src={pic4} alt=\"bike\" width=\"400\"/>\n                        <br/>Each week, Matthew sets aside time to help out fellow dog owners. Whether this means taking them on walks, playing in the backyard, or simply watching over the pooch--Matthew loves it all. <br/><br/> All services offered are done free of charge. Log in to view fellow dog owner's experiences with Matthew's Pet Services, get in contact with Matthew to set up a service, or leave a review of your own. <br/><br/>\n                    </div>\n                </div>\n                <div className={styles.home}>\n                    ~<br/>\"Here For All Your Four-Legged Friend's Needs\"<br/>~\n                </div>  \n                <div className={styles.footer}>\n                    Utilized with <em><a href=\"https://github.com/tuftsm/pets.git\" target=\"_blank\">Github Repo</a></em>&emsp;&emsp;&emsp;\n                    All non-Matthew images licensed for use with <em>Creative Commons</em> licensing. The creator of this website does not claim to own any non-Matthew images.<br/>\n                </div>   \n            </div>\n        </home>\n    );\n};\n\n\nexport default About;","/usr/share/caddy/firebase/src/pages/Schedule.js",["70","71","72","73","74"],[],"import React from 'react';\nimport ReactDOM from 'react-dom/client';\nimport styles from './styles.module.css';\nimport pic1 from './images/beach.jpg';\nimport pic2 from './images/mewalking.jpg';\nimport pic3 from './images/fronthall.jpg';\nimport pic4 from './images/night.JPG';\n\nfunction Schedule() {\n    return(\n        <home>\n            <div className={styles.about}>\n                <div className={styles.photoGridContainer}>\n                    <div className={styles.photoGrid}>\n                        <img src={pic2} alt=\"bike\" width=\"400\"/>\n                        <br/><br/> Matthew is currently accepting requests for all pet services.\n                    <div className={styles.sbio}>\n                        <strong><br/>How to Schedule a Service</strong><br/><br/>\n                    </div>\n                        <br/><br/><br/>\n                        <div className=\"cpContainer\">Email: tuftsmatthew@gmail.com <br/><br/> Cell: 801-243-3134 <br/></div>\n                        <br/><br/>Reach out with the following information: <br/><br/>\n                    \n                        &emsp;&emsp;&emsp;1) Name (of human) <br/><br/>&emsp;&emsp;&emsp;2) Name (of dog) <br/><br/> &emsp;&emsp;&emsp;3) Breed <br/><br/> &emsp;&emsp;&emsp;4) Temperament <br/><br/> &emsp;&emsp;&emsp;5) Preferred day/time <br/><br/> &emsp;&emsp;&emsp;6) Desired services to be performed<br/><br/><br/><br/>\n                    </div>\n                </div>\n                <div className={styles.home}>\n                    ~<br/>\"Here For All Your Four-Legged Friend's Needs\"<br/>~\n                </div>  \n                <div className={styles.footer}>\n                    Utilized with <em><a href=\"https://github.com/tuftsm/pets.git\" target=\"_blank\">Github Repo</a></em>&emsp;&emsp;&emsp;\n                    All non-Matthew images licensed for use with <em>Creative Commons</em> licensing. The creator of this website does not claim to own any non-Matthew images.<br/>\n                </div>   \n            </div>\n        </home>\n    );\n};\n\n\nexport default Schedule;",{"ruleId":"75","severity":1,"message":"76","line":30,"column":39,"nodeType":"77","messageId":"78","endLine":30,"endColumn":100,"fix":"79"},{"ruleId":"80","severity":1,"message":"81","line":27,"column":6,"nodeType":"82","endLine":27,"endColumn":8,"suggestions":"83"},{"ruleId":"75","severity":1,"message":"76","line":59,"column":39,"nodeType":"77","messageId":"78","endLine":59,"endColumn":100,"fix":"84"},{"ruleId":"80","severity":1,"message":"85","line":33,"column":6,"nodeType":"82","endLine":33,"endColumn":8,"suggestions":"86"},{"ruleId":"75","severity":1,"message":"76","line":68,"column":39,"nodeType":"77","messageId":"78","endLine":68,"endColumn":100,"fix":"87"},{"ru